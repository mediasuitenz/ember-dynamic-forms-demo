{{#if display}}
  {{#if formElement.help}}
    <div>{{formElement.help}}</div>
  {{/if}}
  {{#each values as |value index|}}
    {{#unless (or value.hidden value.deleted)}}
      {{dynamic-form/text/text-field
        formElement=formElement
        state=state
        index=index
        updateState=updateState
      }}
      {{#if showDeleteButton}}
        <button {{action 'delete' index}} class="delete">Delete</button>
      {{/if}}
    {{/unless}}
  {{/each}}
  {{#if formElement.repeatable}}
    <button {{action 'add'}}>Add</button>
  {{/if}}
{{/if}}
