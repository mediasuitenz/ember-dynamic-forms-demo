{{#if display}}
  {{#if formElement.help}}
    <div>{{formElement.help}}</div>
  {{/if}}
  {{#each values as |value index|}}
    {{mediasuite-check-box
      checked=value
      label=formElement.label
      update=(action 'updateState' index)
    }}
  {{/each}}
  {{#if formElement.repeatable}}
    <button {{action 'add'}}>Add</button>
  {{/if}}
{{/if}}
